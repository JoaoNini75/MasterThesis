let test1_outer () : int =
  let x = 10 in

  let test1_inner (i : int) : int =
    (x * i)
  (*@ ensures true *)
  in
    
  let i = 2 in
  test1_inner (i)
(*@ ensures true *)

let funs_nested (x) =

  let fun_inner (i) =
    let i = ref (0) in

    while (!i < 3) do
      i := (!i + 2);
      i := (!i - 1)
    done;

    !i
  (*@ ensures true *)
  in
    
  fun_inner (x)
(*@ ensures true *)


