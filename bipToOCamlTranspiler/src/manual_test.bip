let counting_bip (|_ target : int _|, |_ multiple_of _|) : |_ int _| = begin
  let counter = |_ ref 0 _| in

  while (!counter < target) <|> (!counter < target) .
        (!counter mod multiple_of = 0) <|> (!counter mod multiple_of = 0) do
    counter := |_ !counter + 1 _|
  done;

  |_ !counter _|
end
